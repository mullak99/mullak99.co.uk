{
	"name": "mullak99.co.uk",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"dev": "tsc && vite dev --host",
		"build": "yarn update-browsers && vite build && yarn postbuild",
		"postbuild": "npx svelte-sitemap --domain https://mullak99.co.uk",
		"preview": "vite preview",
		"update-browsers": "npx update-browserslist-db@latest",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^7.*",
		"@fortawesome/free-brands-svg-icons": "^7.*",
		"@fortawesome/free-regular-svg-icons": "^7.*",
		"@fortawesome/free-solid-svg-icons": "^7.*",
		"@fortawesome/svelte-fontawesome": "^0.2.*"
	},
	"devDependencies": {
		"@playwright/test": "^1.*",
		"@sveltejs/adapter-auto": "^7.*",
		"@sveltejs/adapter-static": "^3.*",
		"@sveltejs/enhanced-img": "^0.*",
		"@sveltejs/kit": "^2.*",
		"@sveltejs/vite-plugin-svelte": "^6.*",
		"@tailwindcss/postcss": "^4.*",
		"@types/eslint": "^9.*",
		"eslint": "^9.*",
		"eslint-config-prettier": "^10.*",
		"eslint-plugin-svelte": "^3.*",
		"globals": "^16.*",
		"postcss": "^8.*",
		"prettier": "^3.*",
		"prettier-plugin-svelte": "^3.*",
		"svelte": "^5.*",
		"svelte-check": "^4.*",
		"svelte-preprocess": "^6.*",
		"svelte-sitemap": "^2.*",
		"tailwindcss": "^4.*",
		"typescript": "^5.*",
		"typescript-eslint": "^8.*",
		"vite": "^7.*"
	},
	"resolutions": {
		"nanoid": "^5.*",
		"cookie": "^1.*",
		"js-yaml": "^4.*"
	},
	"type": "module",
	"packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
